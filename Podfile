  use_frameworks!

  # Pods for OfflineGPSTracker_iOS
  pod 'Alamofire', '~> 5.4'
  pod 'SwiftyJSON', '~> 5.0'

  target 'OfflineGPSTracker_iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OfflineGPSTracker_iOSUITests' do
    inherit! :search_paths
    # Pods for UI testing
  end
end
